{
  "name": "double-ui",
  "version": "1.0.6",
  "description": "react ui组件库",
  "main": "lib/index.js",
  "author": "xiongshuang",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./lib",
    "clean:prod": "rimraf ./production",
    "dev": "cross-env NODE_ENV=development webpack serve --mode development --config ./config/dev.js",
    "prod": "npm run clean:prod && cross-env NODE_ENV=production webpack --mode production --config ./config/prod.js",
    "components": "cross-env NODE_ENV=production webpack --mode production --config ./config/components.js",
    "libIndex": "node ./config/libIndex.js",
    "single": "cross-env NODE_ENV=production webpack --mode production --config ./config/single.js",
    "baseCss": "cross-env NODE_ENV=production webpack --mode production --config ./config/baseCss.js",
    "build:lib": "npm run clean && npm run single && npm run baseCss && npm run libIndex",
    "build": "npm run clean && npm run single && npm run libIndex",
    "addBaseCss": "node ./config/addBaseCss.js",
    "vite": "vite"
  },
  "keywords": [
    "react",
    "ui"
  ],
  "files": [
    "lib/*"
  ],
  "sideEffects": [
    "src/assets/stylus/main.styl",
    "src/components/base/*",
    "utils/*",
    "*.ts",
    "*.tsx",
    "*.styl",
    "*.css",
    "*.d.ts"
  ],
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/runtime-corejs3": "^7.13.10",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.1",
    "@types/vite-plugin-react-svg": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "core-js": "^3.10.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.1",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "cssnano": "^5.0.1",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.1",
    "loader-utils": "^1.4.0",
    "mini-css-extract-plugin": "^1.4.1",
    "postcss": "^8.2.10",
    "postcss-loader": "^5.2.0",
    "precss": "^4.0.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-svg-loader": "^3.0.3",
    "schema-utils": "^2.7.1",
    "style-loader": "^2.0.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^5.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "vite": "^2.4.4",
    "vite-plugin-react-svg": "^0.2.0",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.2"
  }
}
