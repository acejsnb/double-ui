body
  background-color var(--theme)
  font-family PingFangSC_boldface

.title
  width 100%
  height 48px
  line-height @height
  font-size 14px
  text-align center
  box-shadow var(--box-shadow-bottom)
.route-link
  padding-left 12px
  height 40px
  line-height @height
  font-size 14px
  border-bottom 1px solid var(--grey-200)
.component-view
  padding 20px
